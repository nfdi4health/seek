<%
   status = ApplicationStatus.instance
   unless Seek::Docker.using_docker?
      status.refresh
   end
   search = status.search_enabled ? "enabled" : "disabled"
   soffice = status.soffice_running ? 'running' : 'not running'   
%>
<%= Seek::Config.instance_name %> is running | search is <%= search %> | <%= status.running_jobs %> delayed jobs running | soffice is <%= soffice %>