
<%# adapted _home_activity_notifications removing downloaded items for new landing page %>
<%= panel('Latest additions', body_options: { id: 'recently_added', class: 'front-page-panel' }) do %>
    <ul class="feed">
      <% cache [ActivityLog.where(action:'create').last, current_user_id] do %>
        <% recently_added_item_logs_hash(1.years.ago).each do |added_item_log| -%>
            <%= display_single_log_item_hash(added_item_log, 'added') -%>
        <% end %>
      <% end %>
    </ul>
<% end %>