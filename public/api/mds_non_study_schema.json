    "ID": {
      "additionalProperties": false,
      "properties": {
        "date": {
          "$ref": "#/definitions/date_type"
        },
        "identifier": {
            "$ref": "#/definitions/nonEmptyString"
        },
        "relation_type": {
          "enum": [
            "A is cited by B",
            "A cites B",
            "A is supplement to B",
            "A is supplemented by B",
            "A is continued by B",
            "A continues B",
            "A is described by B",
            "A describes B",
            "A has metadata B",
            "A is metadata for B",
            "A has version B",
            "A is version of B",
            "A is new version of B",
            "A is previous version of B",
            "A is part of B",
            "A has part B",
            "A is referenced by B",
            "A references B",
            "A is documented by B",
            "A documents B",
            "A is compiled by B",
            "A compiles B",
            "A is variant form of B",
            "A is original form of B",
            "A is identical to B",
            "A is reviewed by B",
            "A reviews B",
            "A is derived from B",
            "A is source of B",
            "A is required by B",
            "A requires B",
            "A is obsoleted by  B",
            "A obsoletes B"
