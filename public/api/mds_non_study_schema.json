      "type": "string",
      "minLength": 1
    },
    "date_type": {
      "anyOf": [
        {
          "$ref": "#/definitions/emptyString"
        },
        {
          "type": "string",
          "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
        }
      ]
    },
    "emptyString": {
      "type": "string",
      "minLength": 0,
      "maxLength": 0
