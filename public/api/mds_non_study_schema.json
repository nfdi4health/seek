        "resource_non_study_details": {
          "additionalProperties": false,
          "properties": {
            "resource_format": {
              "type": "string"
            },
            "resource_use_rights": {
              "additionalProperties": false,
              "properties": {
                "resource_use_rights_confirmations": {
                  "additionalProperties": false,
                  "properties": {
                    "resource_use_rights_authors_confirmation_1": {
                      "type": "boolean"
                    },
                    "resource_use_rights_authors_confirmation_2": {
                      "type": "boolean"
                    },
                    "resource_use_rights_authors_confirmation_3": {
                      "type": "boolean"
                    },
                    "resource_use_rights_support_by_licencing": {
                      "type": "boolean"
                    }
                  },
                  "required": [
                    "resource_use_rights_authors_confirmation_1",
                    "resource_use_rights_authors_confirmation_2",
                    "resource_use_rights_authors_confirmation_3",
                    "resource_use_rights_support_by_licencing"
                  ],
                  "type": "object"
                },
                "resource_use_rights_description": {
                  "type": "string"
                },
                "resource_use_rights_label": {
                  "enum": [
                    "CC0 1.0 (Creative Commons Zero v1.0 Universal)",
                    "CC BY 4.0 (Creative Commons Attribution 4.0 International)",
                    "CC BY-NC 4.0 (Creative Commons Attribution Non Commercial 4.0 International)",
                    "CC BY-SA 4.0 (Creative Commons Attribution Share Alike 4.0 International)",
                    "CC BY-NC-SA 4.0 (Creative Commons Attribution Non Commercial Share Alike 4.0 International)",
                    "All rights reserved",
                    "Other",
                    "Not applicable"
                  ],
                  "type": "string"
                }
              },
              "required": [
                "resource_use_rights_label"
              ],
              "type": "object"
            },
            "resource_version": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "resource_titles": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "language": {
                "enum": [
                  "EN (English)",
                  "DE (German)",
                  "ES (Spanish)",
                  "FR (French)",
                  "Other"
                ],
                "type": "string"
              },
              "text": {
               "$ref": "#/definitions/nonEmptyString"
              }
            },
            "required": [
              "text",
              "language"
            ],
            "type": "object"
          },
          "minItems": 1,
          "type": "array"
        },
        "resource_web_page": {
                "anyOf": [
                  {
                    "type": "string",
                    "format": "uri"
                  },
                  {
                    "$ref": "#/definitions/emptyString"
                  }
                ]
        },
        "roles": {
          "items": {
            "$ref": "#/definitions/Role"
          },
          "minItems": 1,
          "type": "array"
        }
      },
      "required": [
        "provenance",
        "resource_classification",
        "resource_description_english",
        "resource_non_study_details",
        "resource_titles",
        "roles"
      ],
      "type": "object"
