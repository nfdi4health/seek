    "NonStudyResource": {
      "additionalProperties": false,
      "properties": {
        "ids": {
          "items": {
            "$ref": "#/definitions/ID"
          },
          "type": "array"
        },
        "ids_alternative": {
          "items": {
            "$ref": "#/definitions/altId"
          },
          "type": "array"
        },
        "ids_nfdi4health": {
          "items": {
            "$ref": "#/definitions/nfdiId"
          },
          "type": "array"
        },
        "provenance": {
          "$ref": "#/definitions/provenance"
        },
        "resource_acronyms": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "language": {
                "enum": [
                  "EN (English)",
                  "DE (German)",
                  "ES (Spanish)",
                  "FR (French)",
                  "Other"
                ],
                "type": "string"
              },
              "text": {
               "$ref": "#/definitions/nonEmptyString"
              }
            },
            "required": [
              "text",
              "language"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "resource_classification": {
          "additionalProperties": false,
          "properties": {
            "resource_type": {
              "enum": [
                "Study",
                "Substudy/Data collection event",
                "Dataset",
                "Study protocol",
                "Protocol amendment",
                "Data dictionary",
                "Informed consent form",
                "Patient information sheet",
                "Manual of operations (SOPs)",
                "Statistical analysis plan",
                "Data management plan",
                "Case report form",
                "Code book",
                "Questionnaire",
                "Interview scheme and themes",
                "Observation guide",
                "Discussion guide",
                "Participant tasks",
                "Other data collection instrument",
                "Other study document",
                "Other"
              ],
              "type": "string"
            },
            "resource_type_general": {
              "enum": [
                "Audiovisual",
                "Book",
                "Book chapter",
                "Collection",
                "Computational notebook",
                "Conference paper",
                "Conference proceeding",
                "Data paper",
                "Dataset",
                "Dissertation",
                "Event",
                "Image",
                "Interactive resource",
                "Journal",
                "Journal article",
                "Model",
