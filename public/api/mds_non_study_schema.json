                "Sponsor (secondary)",
                "Sponsor-Investigator",
                "Research group",
                "Data collector",
                "Data curator",
                "Data manager",
                "Distributor",
                "Hosting institution",
                "Producer",
                "Publisher",
                "Registration agency",
                "Registration authority",
                "Rights holder",
